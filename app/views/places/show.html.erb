<h1><%= @place.name %></h1>

<h2>My Entries</h2>
<% if @entries.any? %>
  <ul>
    <% @entries.each do |entry| %>
      <li>
        <strong><%= entry.title %></strong> - <%= entry.occurred_on %>
        <p><%= entry.description %></p>
      </li>
    <% end %>
  </ul>
<% else %>
  <p>No entries yet.</p>
<% end %>
<%= link_to "Add Entry", new_place_entry_path(@place), class: "btn btn-primary" %>